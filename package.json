{
  "name": "vsus",
  "displayName": "VSUS",
  "publisher": "VSUS",
  "description": "Feel the VSUS, Fall for VSUS ",
  "icon": "VSUSlogo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rajendrakhanal/vsus"
  },
  "version": "2.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:vsus.welcome"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsus.showInfoNotification",
        "title": "Information Notification"
      },
      {
        "command": "vsus.showWarningNotification",
        "title": "Warning Notification"
      },
      {
        "command": "vsus.showErrorNotification",
        "title": "Error Notification"
      },
      {
        "command": "vsus.startup",
        "title": "VSUS"
      },
      {
        "command": "vsus.createCProject",
        "title": "Create Project: C"
      },
      {
        "command": "vsus.createCPPProject",
        "title": "Create Project: C++"
      },
      {
        "command": "vsus.createReactProject",
        "title": "Create Project: React"
      },
      {
        "command": "vsus.createNodeProject",
        "title": "Create Project: NodeJS"
      },
      {
        "command": "vsus.createdjangoProject",
        "title": "Create Project: django"
      },
      {
        "command": "askVSUS.start",
        "title": "AskVSUS"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "askVSUS.start",
          "when": "editorTextFocus",
          "group": "askVSUS"
        },
        {
          "command": "vsus.createCProject",
          "when": "editorTextFocus",
          "group": "menu@1"
        },
        {
          "command": "vsus.createCPPProject",
          "when": "editorTextFocus",
          "group": "menu@2"
        },
        {
          "command": "vsus.createReactProject",
          "when": "editorTextFocus",
          "group": "menu@3"
        },
        {
          "command": "vsus.createNodeProject",
          "when": "editorTextFocus",
          "group": "menu@4"
        },
        {
          "command": "vsus.createdjangoProject",
          "when": "editorTextFocus",
          "group": "menu@5"
        }
      ],
      "snippets": [
        {
          "language": "c",
          "path": "./snippets/snippets-c.json"
        },
        {
          "language": "cpp",
          "path": "./snippets/snippets-c.json"
        },
        {
          "language": "cpp",
          "path": "./snippets/snippets-cpp.json"
        },
        {
          "language": "csharp",
          "path": "./snippets/snippets-csharp.json"
        },
        {
          "language": "css",
          "path": "./snippets/snippets-css.json"
        },
        {
          "language": "dart",
          "path": "./snippets/snippets-dart.json"
        },
        {
          "language": "go",
          "path": "./snippets/snippets-go.json"
        },
        {
          "language": "html",
          "path": "./snippets/snippets-html.json"
        },
        {
          "language": "java",
          "path": "./snippets/snippets-java.json"
        },
        {
          "language": "julia",
          "path": "./snippets/snippets-julia.json"
        },
        {
          "language": "latex",
          "path": "./snippets/snippets-latex.json"
        },
        {
          "language": "lua",
          "path": "./snippets/snippets-lua.json"
        },
        {
          "language": "markdown",
          "path": "./snippets/snippets-markdown.json"
        },
        {
          "language": "php",
          "path": "./snippets/snippets-php.json"
        },
        {
          "language": "r",
          "path": "./snippets/snippets-r.json"
        },
        {
          "language": "ruby",
          "path": "./snippets/snippets-ruby.json"
        },
        {
          "language": "rust",
          "path": "./snippets/snippets-rust.json"
        },
        {
          "language": "sql",
          "path": "./snippets/snippets-sql.json"
        },
        {
          "language": "swift",
          "path": "./snippets/snippets-swift.json"
        },
        {
          "language": "javascript",
          "path": "./snippets/snippets-javascript.json"
        },
        {
          "language": "typescript",
          "path": "./snippets/snippets-typescript.json"
        },
        {
          "language": "javascriptreact",
          "path": "./snippets/snippets-react.json"
        },
        {
          "language": "python",
          "path": "./snippets/snippets-python.json"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "askVSUS",
          "title": "askVSUS",
          "icon": "resources/try.svg"
        }
      ]
    },
    "views": {
      "askVSUS": [
        {
          "type": "webview",
          "id": "askVSUS.chatView",
          "name": "askVSUS"
        }
      ]
    },
    "configuration": {
      "title": "askVSUS",
      "type": "object",
      "properties": {
        "askVSUS.apiKey": {
          "type": "string",
          "markdownDescription": "API key from [OpenAI](https://beta.openai.com/account/api-keys)",
          "order": 0
        },
        "askVSUS.model": {
          "type": "string",
          "enum": [
            "text-davinci-003",
            "text-curie-001",
            "code-davinci-002",
            "code-cushman-002"
          ],
          "default": "text-davinci-003",
          "description": "Which GPT3 model to use",
          "order": 1
        },
        "askVSUS.maxTokens": {
          "type": "number",
          "default": 500,
          "description": "Maximum number of tokens inside response per request",
          "order": 2
        },
        "askVSUS.temperature": {
          "type": "number",
          "default": 0.7,
          "description": "Temperature to use for the GPT3 (between 0 (conservative) and 1 (creative))",
          "order": 3
        },
        "askVSUS.pasteOnClick": {
          "type": "boolean",
          "default": true,
          "description": "Paste the code from a codeblock inside the response into the editor when you click on it",
          "order": 4
        },
        "askVSUS.promptPrefix.explain": {
          "type": "string",
          "default": "Explain what this code does: ",
          "description": "The prompt prefix used for explaining the selected code",
          "order": 5
        },
        "askVSUS.promptPrefix.refactor": {
          "type": "string",
          "default": "Refactor this code and then explain what's changed: ",
          "description": "The prompt prefix used for refactoring the selected code",
          "order": 6
        },
        "askVSUS.promptPrefix.findProblems": {
          "type": "string",
          "default": "Find problems with the following code, fix them and explain what was wrong (Do not change anything else, if there are no problems say so): ",
          "description": "The prompt prefix used for finding problems in the selected code",
          "order": 7
        },
        "askVSUS.promptPrefix.optimize": {
          "type": "string",
          "default": "Optimize the following code if there is anything to improve, if not say so: ",
          "description": "The prompt prefix used for optimizing the selected code",
          "order": 8
        },
        "askVSUS.promptPrefix.documentation": {
          "type": "string",
          "default": "Write documentation for the following code: ",
          "description": "The prompt prefix used for writing documentation for the selected code",
          "order": 9
        },
        "askVSUS.selectedInsideCodeblock": {
          "type": "boolean",
          "default": true,
          "description": "Append selected code as a codeblock (```...code...```) instead of plain text",
          "order": 10
        }
      }
    },
    "scripts": {
      "vscode:prepublish": "npm run package",
      "compile": "webpack",
      "watch": "webpack --watch",
      "package": "webpack --mode production --devtool hidden-source-map",
      "compile-tests": "tsc -p . --outDir out",
      "watch-tests": "tsc -p . -w --outDir out",
      "pretest": "npm run compile-tests && npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
      "menu@1ypes/glob": "^8.0.0",
      "menu@1ypes/mocha": "^10.0.1",
      "menu@1ypes/node": "16.x",
      "menu@1ypes/vscode": "^1.74.0",
      "menu@1ypescript-eslint/eslint-plugin": "^5.45.0",
      "menu@1ypescript-eslint/parser": "^5.45.0",
      "menu@1scode/test-electron": "^2.2.0",
      "eslint": "^8.28.0",
      "glob": "^8.0.3",
      "mocha": "^10.1.0",
      "ts-loader": "^9.4.1",
      "typescript": "^4.9.3",
      "webpack": "^5.75.0",
      "webpack-cli": "^5.0.0"
    },
    "dependencies": {
      "menu@1ypes/fs-extra": "^11.0.0",
      "dotenv": "^16.0.3",
      "fs-extra": "^11.1.0",
      "openai": "^3.1.0",
      "path": "^0.12.7"
    }
  }
}