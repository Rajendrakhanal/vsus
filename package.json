{
  "name": "vsus",
  "displayName": "VSUS",
  "publisher": "VSUS",
  "description": "Feel the VSUS, Fall for VSUS ",
  "icon": "VSUSlogo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rajendrakhanal/vsus"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsus.welcome"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsus.showInfoNotification",
        "title": "Information Notification"
      },
      {
        "command": "vsus.showWarningNotification",
        "title": "Warning Notification"
      },
      {
        "command": "vsus.showErrorNotification",
        "title": "Error Notification"
      },
      {
        "command": "vsus.startup",
        "title": "VSUS"
      },
      {
        "command": "vsus.createCProject",
        "title": "Create Project: C"
      },
      {
        "command": "vsus.createCPPProject",
        "title": "Create Project: C++"
      },
      {
        "command": "vsus.createReactProject",
        "title": "Create Project: React"
      },
      {
        "command": "vsus.createNodeProject",
        "title": "Create Project: NodeJS"
      },
      {
        "command": "vsus.createdjangoProject",
        "title": "Create Project: django"
      },
      {
        "command": "askVSUS.start",
        "title": "AskVSUS"
      }
    ],
    "snippets": [
      {
        "language": "c",
        "path": "./src/json/snippets/c.json"
      },
      {
        "language": "cpp",
        "path": "./src/json/snippets/cpp.json"
      },
      {
        "language": "csharp",
        "path": "./src/json/snippets/csharp.json"
      },
      {
        "language": "css",
        "path": "./src/json/snippets/css.json"
      },
      {
        "language": "dart",
        "path": "./src/json/snippets/dart.json"
      },
      {
        "language": "go",
        "path": "./src/json/snippets/go.json"
      },
      {
        "language": "html",
        "path": "./src/json/snippets/html.json"
      },
      {
        "language": "java",
        "path": "./src/json/snippets/java.json"
      },
      {
        "language": "julia",
        "path": "./src/json/snippets/julia.json"
      },
      {
        "language": "latex",
        "path": "./src/json/snippets/latex.json"
      },
      {
        "language": "lua",
        "path": "./src/json/snippets/lua.json"
      },
      {
        "language": "markdown",
        "path": "./src/json/snippets/markdown.json"
      },
      {
        "language": "php",
        "path": "./src/json/snippets/php.json"
      },
      {
        "language": "r",
        "path": "./src/json/snippets/r.json"
      },
      {
        "language": "ruby",
        "path": "./src/json/snippets/ruby.json"
      },
      {
        "language": "rust",
        "path": "./src/json/snippets/rust.json"
      },
      {
        "language": "sql",
        "path": "./src/json/snippets/sql.json"
      },
      {
        "language": "swift",
        "path": "./src/json/snippets/swift.json"
      },
      {
        "language": "javascript",
        "path": "./src/json/snippets/javascript/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./src/json/snippets/javascript/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/json/snippets/javascript/react.json"
      },
      {
        "language": "python",
        "path": "./src/json/snippets/python/python.json"
      }
    ],
    "configuration": {
      "title": "AskVSUS",
      "type": "object",
      "properties": {
        "askVSUS.apiKey": {
          "type": "string",
          "markdownDescription": "API key from [OpenAI](https://beta.openai.com/account/api-keys)",
          "order": 0
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0",
    "openai": "^3.1.0",
    "path": "^0.12.7"
  }
}
